{
    "name": "sookoll/picu",
    "version": "2.5.1",
    "description": "Personal photo gallery based on Flickr API private photos",
    "homepage": "https://sookoll.ee/picu",
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-fileinfo": "*",
        "bryanjhv/slim-session": "^4.1",
        "lulco/phoenix": "^2.6",
        "monolog/monolog": "^3.6",
        "php-di/php-di": "^7.0",
        "samwilson/phpflickr": "^5.1",
        "slim/psr7": "^1.6",
        "slim/slim": "4.*",
        "slim/twig-view": "3.x-dev",
        "symfony/cache": "^7.0",
        "vlucas/phpdotenv": "^5.6"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080"
    }
}
